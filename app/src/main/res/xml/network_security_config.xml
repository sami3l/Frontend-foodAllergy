<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext only for your local backend IP -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">192.168.11.128</domain>
    </domain-config>

    <!-- Optionally, block cleartext everywhere else -->
    <base-config cleartextTrafficPermitted="false" />
</network-security-config>
